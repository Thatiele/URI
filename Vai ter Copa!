#include <stdio.h>
#include <stdlib.h>

main(){
  
    int num;
     
    while(scanf("%d",&num) !=EOF){//a função while verifica o valor no EOP, sendo então caso de teste
         
        if(num==0) // condição que ve se o numero for igual ou diferente de 0
        printf("vai ter copa!\n");
        else
        printf("vai ter duas!\n");
        }
  
    return 0;

}
